{
    "checkbuild": {
        "enable": ["eslint", "jsinspect", "nsp", "david"],
        "continueOnError": true,
        "allowFailures": false
    },
    "eslint": {
        "args": ["lib/**/*.js", "index.js"]
    },
    "jsinspect": {
        "args": ["lib/**/*.js", "index.js"],
        "diff": true,
        "identifiers": true,
    },
    "nsp": {},
    "david": {
        "dev": false,
        "stable": true,
        "ignore": []
    }
}