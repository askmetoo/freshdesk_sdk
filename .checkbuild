{
    "checkbuild": {
        "enable": ["eslint", "jsinspect", "buddyjs", "nsp", "david"],
        "continueOnError": true,
        "allowFailures": false
    },
    "eslint": {
        "args": ["lib/**"]
    },
    "jsinspect": {
        "args": ["lib/*.js"],
        "diff": true,
        "identifiers": true,
    },
    "buddyjs": {
        "args": ["lib/*.js"],
        "detectObjects": false,
        "enforceConst": false,
    },
    "nsp": {},
    "david": {
        "dev": false,
        "stable": true,
        "ignore": []
    }
}